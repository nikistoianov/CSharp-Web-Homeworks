@model DirectorViewModel
@{
    ViewData["Title"] = "Details";
}

<br />
<h2>@Model.Name</h2>
<ul>
    @foreach (var movie in Model.Movies)
    {
        <li>
            <a asp-controller="Movies" asp-action="Details" asp-route-id="@movie.MovieId">@movie.Title</a> (@movie.Status)
        </li>
    }
</ul>
